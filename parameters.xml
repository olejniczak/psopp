<?xml version="1.0" encoding="utf-8"?>
<optimization>
    <problem>
        <objective_function>(1-x1)^2+100*(x2-x1*x1)^2</objective_function>
        <search_space>
            <minx1>-2</minx1>
            <maxx1>2</maxx1>
            <minx2>-2</minx2>
            <maxx2>2</maxx2>
        </search_space>
    </problem>
    <algorithm>
        <parameters>
            <inertia_weight>0.4</inertia_weight>
            <particle_learn>2</particle_learn>
            <swarm_learn>2</swarm_learn>
        </parameters>        
    </algorithm>
</optimization>
  
